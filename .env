# Web Call Environment Configuration
# Copy this file to .env and modify as needed

# Backend configuration
PUBLIC_BASE_URL=http://localhost:5173

# TURN Server configuration
TURN_USERNAME=user
TURN_PASSWORD=secret
TURN_REALM=localhost
TURN_EXTERNAL_IP=

# Frontend ICE configuration
# This JSON will be embedded into the frontend build
# Format: single-line JSON array of ICE servers
VITE_ICE_JSON=[{"urls":["stun:localhost:3478"]},{"urls":["turn:localhost:3478?transport=udp","turn:localhost:3478?transport=tcp"],"username":"user","credential":"secret"}]

# ICE transport policy: "all" (default) or "relay" (TURN-only)
VITE_ICE_TRANSPORT_POLICY=all

# Optional: Force H.264 codec for better compatibility
VITE_FORCE_H264=false
